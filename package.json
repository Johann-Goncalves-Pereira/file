{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "json-to-scss ./.style-abstracts/_theme.json && qwik build",
    "build.client": "json-to-scss ./.style-abstracts/_theme.json && vite build",
    "build.preview": "json-to-scss ./.style-abstracts/_theme.json && vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "json-to-scss ./.style-abstracts/_theme.json && vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "json-to-scss ./.style-abstracts/_theme.json && qwik build preview && vite preview --open",
    "start": "json-to-scss ./.style-abstracts/_theme.json && vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.1.4",
    "@builder.io/qwik-city": "^1.1.4",
    "@builder.io/qwik-react": "0.5.0",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/material": "5.13.0",
    "@mui/x-data-grid": "6.4.0",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/eslint": "8.37.0",
    "@types/node": "^20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@vitejs/plugin-legacy": "^4.0.4",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "doiuse": "^4.4.1",
    "eslint": "8.40.0",
    "eslint-plugin-qwik": "^1.1.4",
    "json-to-scss": "^1.6.2",
    "postcss": "^8.4.23",
    "postcss-custom-media": "^9.1.3",
    "postcss-custom-selectors": "^7.1.3",
    "postcss-inline-svg": "^6.0.0",
    "postcss-nesting": "^11.2.2",
    "postcss-preset-env": "^8.4.1",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2",
    "terser": "^5.17.6",
    "ts-reset": "^0.0.1",
    "typescript": "5.0.4",
    "undici": "5.22.1",
    "vite": "4.3.5",
    "vite-tsconfig-paths": "4.2.0"
  }
}
